			</theme>
			<theme name="moisture-level2" ref="-defaults">
				<param name="background"><image>water-level.2</image></param>
				<param name="minWidth"><int>50</int></param>
				<param name="minHeight"><int>15</int></param>
			</theme>
			<theme name="moisture-level3" ref="-defaults">
				<param name="background"><image>water-level.3</image></param>
				<param name="minWidth"><int>50</int></param>
				<param name="minHeight"><int>15</int></param>
			</theme>
			<theme name="moisture-level4" ref="-defaults">
				<param name="background"><image>water-level.4</image></param>
				<param name="minWidth"><int>50</int></param>
				<param name="minHeight"><int>15</int></param>
			</theme>
          <theme name="moisture-level5" ref="-defaults">
            <param name="background">
              <image>water-level.5</image>
            </param>
            <param name="minWidth">
              <int>50</int>
            </param>
            <param name="minHeight">
              <int>15</int>
            </param>
          </theme>
          <theme name="moisture-level6" ref="-defaults">
            <param name="background">
              <image>water-level.blue</image>
            </param>
            <param name="minWidth">
              <int>50</int>
            </param>
            <param name="minHeight">
              <int>15</int>
            </param>
          </theme>
          <theme name="misc-nameplate-nobg" ref="-defaults">
            <param name="background">
              <image>water-level.background</image>
            </param>
          </theme>
          <param name="font">
            <font>alphabeta-border</font>
          </param>
        </theme>
		<theme name="nameplate-friend" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta-border-friend</font></param>
		</theme>
		<theme name="nameplate-team" ref="-defaults" allowWildcard="true">
			<param name="font"><font>alphabeta-border-team</font></param>
		</theme>
		<theme name="message" ref="-defaults" allowWildcard="true">
			<param name="background"><image>chatframe.background</image></param>
			<param name="font"><font>pb-dark</font></param>
		</theme>
		<!-- Chat Bubbles -->
		<theme name="chat-bubble">
			<param name="font"><font>tooltip-font</font></param>
			<param name="background"><image>chat-bubble.background</image></param>
			<param name="border"><border>2,5,8,3</border></param>
			<param name="minWidth"><int>20</int></param>
			<param name="minHeight"><int>16</int></param>
			<param name="maxWidth"><int>200</int></param>
			<param name="maxHeight"><int>600</int></param>
			<param name="overlay"><image>none</image></param>
			<param name="fonts">
				<map>
					<param name="default"><font>tooltip-font</font></param>
